{
    "name": "ng2-brpipes",
    "version": "8.0.0",
    "description": "Angular2 pipes for BR types",
    "files": [
        "src/*.d.ts",
        "src/*.js",
        "src/*.metadata.json",
        "!src/**/*.ngsummary.json",
        "index.d.ts",
        "tsconfig.json"
    ],
    "main": "src/index.js",
    "typings": "index.d.ts",
    "scripts": {
        "prepublish": "node_modules/.bin/gulp clean && npm run ngc",
        "tsc": "node_modules/.bin/tsc -p .",
        "karma": "node_modules/.bin/karma start karma.conf.js",
        "test": "node_modules/.bin/gulp clean && npm run ngc:test && npm run karma",
        "ngc": "node_modules/.bin/ngc -p tsconfig.json",
        "ngc:test": "node_modules/.bin/ngc -p tsconfig.test.json"
    },
    "author": "Filipe Augusto Lima de Souza",
    "repository": {
        "type": "git",
        "url": "https://github.com/fals/ng2-brpipes"
    },
    "keywords": [
        "ng2",
        "ng",
        "angular",
        "angular2",
        "angularjs",
        "pipes",
        "filters",
        "library"
    ],
    "license": "MIT",
    "devDependencies": {
        "@angular/compiler": "~8.0.1",
        "@angular/compiler-cli": "~8.0.1",
        "@angular/core": "~8.0.1",
        "@angular/platform-server": "~8.0.1",
        "@types/core-js": "2.5.2",
        "@types/jasmine": "3.3.13",
        "core-js": "3.1.4",
        "del": "4.1.1",
        "gulp": "4.0.2",
        "gulp-clang-format": "1.0.27",
        "jasmine-core": "3.4.0",
        "karma": "4.1.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-jasmine": "2.0.1",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "4.0.2",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.2",
        "symbol-observable": "1.2.0",
        "typescript": "3.4.5",
        "webpack": "4.34.0",
        "zone.js": "0.9.1"
    },
    "dependencies": {
        "npm": "^6.11.3"
    }
}
