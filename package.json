{
    "name": "ng2-brpipes",
    "version": "1.1.4",
    "description": "Angular2 pipes for BR types",
    "files": [
        "src/*.d.ts",
        "src/*.js",
        "src/*.metadata.json",
        "!src/**/*.ngsummary.json",
        "index.d.ts",
        "tsconfig.json"
    ],
    "main": "src/index.js",
    "typings": "index.d.ts",
    "scripts": {
        "prepublish": "node_modules/.bin/gulp clean && npm run ngc",
        "tsc": "node_modules/.bin/tsc -p .",
        "karma": "node_modules/.bin/karma start karma.conf.js",
        "test": "node_modules/.bin/gulp clean && npm run ngc:test && npm run karma",
        "ngc": "node_modules/.bin/ngc -p tsconfig.json",
        "ngc:test": "node_modules/.bin/ngc -p tsconfig.test.json"
    },
    "author": "Filipe Augusto Lima de Souza",
    "repository": {
        "type": "git",
        "url": "https://github.com/fals/ng2-brpipes"
    },
    "keywords": [
        "ng2",
        "ng",
        "angular",
        "angular2",
        "angularjs",
        "pipes",
        "filters",
        "library"
    ],
    "license": "MIT",
    "devDependencies": {
        "@angular/compiler": "~2.1.1",
        "@angular/compiler-cli": "~2.1.1",
        "@angular/core": "~2.1.1",
        "@angular/platform-server": "~2.1.1",
        "@types/core-js": "0.9.34",
        "@types/jasmine": "2.5.37",
        "core-js": "^2.4.1",
        "del": "^2.2.2",
        "gulp": "^3.9.1",
        "gulp-clang-format": "^1.0.23",
        "jasmine-core": "^2.4.1",
        "karma": "^0.13.22",
        "karma-chrome-launcher": "^2.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-spec-reporter": "0.0.26",
        "karma-webpack": "^1.8.0",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-rc.4",
        "symbol-observable": "^0.2.4",
        "typescript": "2.0.8",
        "webpack": "^1.13.3",
        "zone.js": "0.7.2"
    },
    "dependencies": {
        "npm": "^5.7.1"
    }
}
